pipeline {
  agwnt any
  stages {
      stage("build") {
          steps {
              echo 'bulding app'
          }
      }
            stage("test") {
          steps {
              echo 'testing app'
          }
      }
            stage("deploy") {
          steps {
              echo 'deploy app'
          }
      }
            stage("deploy") {
          steps {
              javac hello_java.java
          }
      }
  }
}
