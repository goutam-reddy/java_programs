pipeline {
    agent any
    stages {
        stage("build") {
            steps {
                sh 'echo bulding app'
            }
        }
        stage("test") {
            steps {
                sh 'echo "testing app"'
            }
        }
        stage("deploy") {
            steps {
                sh 'echo "deploying app"'
            }
        }
    }
}
